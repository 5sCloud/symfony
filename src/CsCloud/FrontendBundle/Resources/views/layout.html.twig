<!doctype html>
<html>
    <head>
        <title>{% if (title is defined) and title|trim is not empty %}{{ title|trim }} | CsCloud{% else %}CsCloud{% endif %}</title>
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="format-detection" content="telephone=no">
        <link rel="shortcut icon" href="/favicon.png" />

        {% block meta_robots %}{% endblock %}

        {% stylesheets '@CsCloudFrontendBundle/Resources/assets/less/bootstrap.less'
                       '@CsCloudFrontendBundle/Resources/assets/less/style.less'
                  filter='less,?yui_css' output='css/compiled/main.css' %}
        <link rel="stylesheet" type="text/css" href="{{ asset_url }}" />
        {% endstylesheets %}

        {% block extra_css %}{% endblock %}
        <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
        {% stylesheets '@CsCloudFrontendBundle/Resources/assets/less/ie8.less'
            filter="less" output="css/compiled/ie8.css" %}
            <link rel="stylesheet" type="text/css" href="{{ asset_url }}" />
        {% endstylesheets %}
            <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
            <script src="//cdn.jsdelivr.net/respond/1.4.2/respond.min.js"></script>
            <link href="{{ asset('respond-proxy.html') }}" id="respond-proxy" rel="respond-proxy" />
            <link href="/respond.proxy.gif" id="respond-redirect" rel="respond-redirect" />
            <script src="/respond.proxy.js"></script>
        <![endif]-->
    </head>
    <body class="{% if (body_class is defined) %} {{ body_class }}{% endif %}">

        <nav class="navbar navbar-default" role="navigation">
            <div class="container-fluid">
                <div class="navbar-header">
                    <a href="#_"><img src="{{ asset('/images/Home.png') }}" /></a>
                </div>
                <p class="navbar-text">Nome Cognome - Gruppo 1 Gruppo 2</p>
                <ul class="nav navbar-nav navbar-right">
                    <li><a href="#" rel="tooltip" data-placement="bottom" title="Impostazioni"><i class="glyphicon glyphicon-cog"></i></a></li>
                    <li><a href="#" rel="tooltip" data-placement="bottom" title="Contatti"><i class="glyphicon glyphicon-phone"></i></a></li>
                    <li><a href="#" rel="tooltip" data-placement="bottom" title="Messaggi"><i class="glyphicon glyphicon-comment"></i></a></li>
                    <li><a href="#" rel="tooltip" data-placement="bottom" title="Alert"><i class="glyphicon glyphicon-warning-sign"></i></a></li>
                    <li><a href="#" rel="tooltip" data-placement="bottom" title="Profilo"><i class="glyphicon glyphicon-user"></i></a></li>
                    <li><a href="#" rel="tooltip" data-placement="bottom" title="Help"><i class="glyphicon glyphicon-question-sign"></i></a></li>
                </ul>
                <form class="navbar-form navbar-right" role="search">
                    <div class="form-group">
                        <input type="text" class="form-control" placeholder="Ricerca" />
                    </div>
                    <button type="submit" class="btn btn-default">Cerca</button>
                </form>
            </div>
        </nav>

        <div class="container">
            <div class="col-md-3 col-xs-4">
                <ul class="nav nav-pills nav-stacked">
                    <li class="active"><a href='#file'>File</a></li>
                    <li><a href='#attivita'>Attivit√†</a></li>
                    <li><a href='#documenti'>Documenti</a></li>
                    <li><a href='#sheet'>Fogli di Calcolo</a></li>
                    <li><a href='#picture'>Immagini</a></li>
                    <li><a href='#news'>News</a></li>
                    <li><a href='#bookmark'>Bookmark</a></li>
                    <li><a href='#calendario'>Calendario</a></li>
                    <li><a href='#contatti'>Contatti</a></li>
                    <li role="presentation" class="divider"></li>
                    <li><a href='#profilo'>Profilo</a></li>
                    <li><a href='#settaggi'>Settaggi</a></li>
                    <li role="presentation" class="divider"></li>
                    <li><a href='#uscita'>uscita</a></li>
                </ul>
            </div>
            <div class="col-md-9 col-xs-8 breadcrumbs">{% block breadcrumbs %} BREADCRUMBS {% endblock %}</div>
            <div class="col-md-9 col-xs-8 clearfix main-container container-fluid">
                {% block body %}
                    MAIN CONTENT
                {% endblock %}
            </div>
        </div>

        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
        {% javascripts 
                       '@CsCloudFrontendBundle/Resources/assets/js/bootstrap/modal.js'
                       '@CsCloudFrontendBundle/Resources/assets/js/bootstrap/tooltip.js'
                       '@CsCloudFrontendBundle/Resources/assets/js/main.js'
            filter="?closure" output="js/compiled/main.js" %}
            <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
        {% block extra_js %}{% endblock %}
    </body>
</html>
